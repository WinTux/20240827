@model ProyectoAppWeb.Models.Cuenta2

<h3>Registro de cuenta</h3>
<form asp-controller="FormValidacion" asp-action="Guardar" method="post">
    <table class="table">
        <tr>
            <td>Usuario</td>
            <td>
                <input asp-for="usuario">
            </td>
            <td>
                <span asp-validation-for="usuario"></span>
            </td>
        </tr>
        <tr>
            <td>Password</td>
            <td>
                <input type="password" asp-for="password">
            </td>
            <td>
                <span asp-validation-for="password"></span>
            </td>
        </tr>
        <tr>
            <td>Edad</td>
            <td>
                <input asp-for="edad">
            </td>
            <td>
                <span asp-validation-for="edad"></span>
            </td>
        </tr>
        <tr>
            <td>email</td>
            <td>
                <input asp-for="email">
            </td>
            <td>
                <span asp-validation-for="email"></span>
            </td>
        </tr>
        <tr>
            <td>Página</td>
            <td>
                <input asp-for="pagina">
            </td>
            <td>
                <span asp-validation-for="pagina"></span>
            </td>
        </tr>
        <tr>
            <td></td>
            <td><input type="submit" value="Registrar"></td>
        </tr>
    </table>
</form>