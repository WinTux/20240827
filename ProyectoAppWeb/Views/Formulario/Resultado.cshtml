<h3>Datos de la cuenta creada</h3>
<table class="table">
    <tr>
        <td>ID</td>
        <td>
            @ViewBag.cuenta.Id
        </td>
    </tr>
    <tr>
        <td>Usuario</td>
        <td>
            @ViewBag.cuenta.Usuario
        </td>
    </tr>
    <tr>
        <td>Password</td>
        <td>
            @ViewBag.cuenta.Password
        </td>
    </tr>
    <tr>
        <td>Descripcion</td>
        <td>
            @ViewBag.cuenta.Descripcion
        </td>
    </tr>
    <tr>
        <td>Disponible</td>
        <td>
            @ViewBag.cuenta.Disponible
        </td>
    </tr>
    <tr>
        <td>Genero</td>
        <td>
            @ViewBag.cuenta.Genero
        </td>
    </tr>
    <tr>
        <td>Cargo</td>
        <td>
            @ViewBag.cuenta.Cargo
        </td>
    </tr>
    <tr>
        <td>Lenguajes de programacion</td>
        <td>
            @foreach (var len in ViewBag.cuenta.Lenguajes)
            {
                <span>@len</span><br>
            }
        </td>
    </tr>
</table>